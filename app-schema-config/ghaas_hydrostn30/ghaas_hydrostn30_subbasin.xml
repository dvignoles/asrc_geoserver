<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess xmlns:as="http://www.geotools.org/app-schema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.geotools.org/app-schema AppSchemaDataAccess.xsd">
    <namespaces>
        <Namespace>
            <prefix>gml</prefix>
            <uri>http://www.opengis.net/gml</uri>
        </Namespace>
        <Namespace>
            <prefix>ghaas</prefix>
            <uri>ghaas</uri>
        </Namespace>
    </namespaces>
    <sourceDataStores>
        <DataStore>
            <id>datastore</id>
            <parameters>
                <Parameter>
                    <name>dbtype</name>
                    <value>postgisng</value>
                </Parameter>
                <Parameter>
                    <name>host</name>
                    <value>${postgis.host}</value>
                </Parameter>
                <Parameter>
                    <name>port</name>
                    <value>${postgis.port}</value>
                </Parameter>
                <Parameter>
                    <name>database</name>
                    <value>${postgis.database}</value>
                </Parameter>
                <Parameter>
                    <name>user</name>
                    <value>${postgis.user}</value>
                </Parameter>
                <Parameter>
                    <name>passwd</name>
                    <value>${postgis.passwd}</value>
                </Parameter>
                <Parameter>
                    <name>Expose primary keys</name>
                    <value>true</value>
                </Parameter>
            </parameters>
        </DataStore>
    </sourceDataStores>
    <targetTypes>
        <FeatureType>
            <schemaUri>ghaas.xsd</schemaUri>
        </FeatureType>
    </targetTypes>
    <typeMappings>
        <FeatureTypeMapping>
            <sourceDataStore>datastore</sourceDataStore>
            <sourceType>hydrostn30_subbasin</sourceType>
            <targetElement>ghaas:Subbasin</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:gridValue
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>gridvalue</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:gridArea
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>gridarea</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:gridPercent
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>gridpercent</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:perimeter
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>perimeter</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:vertexNum
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>vertexnum</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:basinId
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>basinid</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:streamOrder
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>streamorder</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:fromXCoord
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>fromxcoord</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:fromYCoord
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>fromycoord</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:toXCoord
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>toxcoord</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:toYCoord
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>toycoord</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:cellId
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>cellid</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:basinName
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>basinname</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:order
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>order</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:color
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>color</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:numberOfCells
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>numberofcells</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:stnMainstemLength
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>stnmainstemlength</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:stnCatchmentArea
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>stncatchmentarea</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:stnInterstationArea
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>stninterstationarea</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:nextStation
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>nextstation</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:geom
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>geom</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:Subbasin
                    </targetAttribute>
                    <idExpression>
                        <OCQL>strConcat('Subbasin.', id)</OCQL>
                    </idExpression>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>
    </typeMappings>
</as:AppSchemaDataAccess>