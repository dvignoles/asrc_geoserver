                <Parameter>
                    <name>host</name>
                    <value>${postgis.host}</value>
                </Parameter>
                <Parameter>
                    <name>port</name>
                    <value>${postgis.port}</value>
                </Parameter>
                <Parameter>
                    <name>database</name>
                    <value>${postgis.database}</value>
                </Parameter>
                <Parameter>
                    <name>user</name>
                    <value>${postgis.user}</value>
                </Parameter>
                <Parameter>
                    <name>passwd</name>
                    <value>${postgis.passwd}</value>
                </Parameter>
                <Parameter>
                    <name>Expose primary keys</name>
                    <value>true</value>
                </Parameter>
            </parameters>
        </DataStore>
    </sourceDataStores>
