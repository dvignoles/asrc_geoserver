<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess xmlns:as="http://www.geotools.org/app-schema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.geotools.org/app-schema AppSchemaDataAccess.xsd">
    <namespaces>
        <Namespace>
            <prefix>gml</prefix>
            <uri>http://www.opengis.net/gml</uri>
        </Namespace>
        <Namespace>
            <prefix>ghaas</prefix>
            <uri>ghaas</uri>
        </Namespace>
    </namespaces>
    <sourceDataStores>
        <DataStore>
            <id>datastore</id>
            <parameters>
                <Parameter>
                    <name>dbtype</name>
                    <value>postgisng</value>
                </Parameter>
                <Parameter>
                    <name>host</name>
                    <value>${postgis.host}</value>
                </Parameter>
                <Parameter>
                    <name>port</name>
                    <value>${postgis.port}</value>
                </Parameter>
                <Parameter>
                    <name>database</name>
                    <value>${postgis.database}</value>
                </Parameter>
                <Parameter>
                    <name>user</name>
                    <value>${postgis.user}</value>
                </Parameter>
                <Parameter>
                    <name>passwd</name>
                    <value>${postgis.passwd}</value>
                </Parameter>
                <Parameter>
                    <name>Expose primary keys</name>
                    <value>true</value>
                </Parameter>
            </parameters>
        </DataStore>
    </sourceDataStores>
    <targetTypes>
        <FeatureType>
            <schemaUri>ghaas_mouth.xsd</schemaUri>
        </FeatureType>
    </targetTypes>
    <typeMappings>
        <FeatureTypeMapping>
            <sourceDataStore>datastore</sourceDataStore>
            <sourceType>hydrostn30_mouth</sourceType>
            <targetElement>ghaas:Mouth</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:basinOrder
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>basinorder</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:color
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>color</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:basinLength
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>basinlength</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:basinArea
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>basinarea</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:geom
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>geom</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:Mouth
                    </targetAttribute>
                    <idExpression>
                        <OCQL>strConcat('Mouth.', id)</OCQL>
                    </idExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>ghaas:dischargeValues</targetAttribute>
                    <sourceExpression>
                        <OCQL>id</OCQL>
                        <linkElement>ghaas:Discharge</linkElement>
                        <linkField>FEATURE_LINK</linkField>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>
        <FeatureTypeMapping>
            <sourceDataStore>datastore</sourceDataStore>
            <sourceType>discharge_mouth_annual</sourceType>
            <targetElement>ghaas:Discharge</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:year
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>year</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:dischargeValue
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>discharge</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ghaas:Discharge
                    </targetAttribute>
                    <idExpression>
                        <OCQL>strConcat('Discharge.', id)</OCQL>
                    </idExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>FEATURE_LINK</targetAttribute>
                    <sourceExpression>
                        <OCQL>sampleid</OCQL>
                    </sourceExpression>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>
    </typeMappings>
</as:AppSchemaDataAccess>
